<nav class="navbar navbar-expand-lg navbar-light bg-light">
<a class="navbar-brand" href="#">Tech News Scraper</a>
<div class="collapse navbar-collapse" id="navbarNavAltMarkup">
  <div class="navbar-nav">
    <a class="nav-item nav-link active" href="/">Home</a>
    <a class="nav-item nav-link" href="/saved">Saved articles</a>
    <a class="btn btn-danger scrape-new">Get new articles</a>
  </div>
</div>
</nav>

<div class="jumbotron jumbotron-fluid text-center">
<h1 class="display-4">Tech News</h1>
<p class="lead">Get the lastest tech news</p>
</div>

<div class="container article-container">
{{#each articles}}
<div class="card bg-light mb-3">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text">{{this.intro}}...</p>
    <a href="{{this.link}}" class="card-link" target="_blank">View article</a>
    <a data-id="{{this._id}}" class="btn btn-primary save-btn">Save article</a>
  </div>
</div>
{{/each}}
</div>